{
   "env":
   {
      "ARM_GCC_TOOLCHAIN": "C:/Data/GCC/xpack-arm-none-eabi-gcc"
   },
   "configurations": [
      {
         "name": "ARM",
         "compilerPath": "${env.ARM_GCC_TOOLCHAIN}/bin/arm-none-eabi-gcc.exe",
         "includePath": [
            "${env.ARM_GCC_TOOLCHAIN}/arm-none-eabi/include/**",
            "${env.ARM_GCC_TOOLCHAIN}/arm-none-eabi/include/sys/**",
            "${env.ARM_GCC_TOOLCHAIN}/lib/**",
            "${workspaceRoot}/Drivers/**",
            "${workspaceRoot}/Src/**",
            "${workspaceRoot}/DriversExt/**",
            "${workspaceRoot}/Inc/**"
         ],
         "defines": [
            "GD32E10X",
            "HXTAL_VALUE=8000000U",
            "HXTAL_VALUE_8M=8000000U",
            "M4",
            "GCC_ARMCM4"
         ],
         "cStandard": "c11",
         "cppStandard": "c++17",
         "intelliSenseMode": "windows-gcc-arm"
      }
   ],
   "version": 4
}